[
  {
    "id": "enhanced-high-value-gifts",
    "name": "High Value Donations (Enhanced)",
    "description": "Triggers for high-value donations from YouTube SuperChats over $20 or Bilibili gifts over Â¥50",
    "enabled": true,
    "conditionGroups": [
      {
        "source": "youtube",
        "messageType": "superchat",
        "conditions": [
          {
            "field": "price",
            "operator": "greater_than",
            "value": 20,
            "dataType": "number"
          }
        ],
        "conditionLogic": "AND"
      },
      {
        "source": "bilibili",
        "messageType": "gift",
        "conditions": [
          {
            "field": "price",
            "operator": "greater_than",
            "value": 50,
            "dataType": "number"
          }
        ],
        "conditionLogic": "AND"
      }
    ],
    "groupLogic": "OR",
    "actions": [
      {
        "type": "route_to_endpoint",
        "endpoint": "/onecomme/high-value-donations",
        "fields": ["name", "comment", "price", "giftName", "userLevel", "guardLevel"]
      }
    ],
    "blockDefault": false
  },
  {
    "id": "vip-members-only",
    "name": "VIP Members (Multi-Platform)",
    "description": "Routes messages from VIP/Premium members across all platforms",
    "enabled": true,
    "conditionGroups": [
      {
        "source": "youtube",
        "messageType": "comment",
        "conditions": [
          {
            "field": "isMember",
            "operator": "equals",
            "value": true,
            "dataType": "boolean"
          }
        ],
        "conditionLogic": "AND"
      },
      {
        "source": "bilibili",
        "messageType": "comment",
        "conditions": [
          {
            "field": "isVip",
            "operator": "equals",
            "value": true,
            "dataType": "boolean"
          }
        ],
        "conditionLogic": "OR"
      },
      {
        "source": "bilibili",
        "messageType": "comment",
        "conditions": [
          {
            "field": "guardLevel",
            "operator": "greater_than",
            "value": 0,
            "dataType": "number"
          }
        ],
        "conditionLogic": "AND"
      }
    ],
    "groupLogic": "OR",
    "actions": [
      {
        "type": "route_to_endpoint",
        "endpoint": "/onecomme/vip-members",
        "fields": ["name", "comment", "userLevel", "guardLevel", "isVip", "isMember"]
      }
    ],
    "blockDefault": false
  }
]